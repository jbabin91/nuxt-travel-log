{
  "name": "nuxt-app",
  "version": "0.0.1",
  "private": true,
  "description": "Nuxt app for travel logs",
  "type": "module",
  "scripts": {
    "auth:generate": "pnpm dlx @better-auth/cli@latest generate --config src/lib/auth.ts --output src/lib/db/schema/auth.ts",
    "build": "nuxt build",
    "db:generate": "drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config ./drizzle.config.ts",
    "dev": "concurrently \"pnpm run dev:db\" \"nuxt dev\"",
    "dev:db": "turso dev --db-file local.db",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@indoorequal/vue-maplibre-gl": "^8.4.2",
    "@libsql/client": "^0.15.15",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/icon": "1.10.3",
    "@nuxtjs/color-mode": "3.5.2",
    "@pinia/nuxt": "0.11.2",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/zod": "^4.15.1",
    "better-auth": "^1.3.27",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "eslint": "^9.27.0",
    "maplibre-gl": "^5.5.0",
    "nanoid": "^5.1.5",
    "nuxt": "^3.17.4",
    "nuxt-csurf": "1.6.5",
    "nuxt-maplibre": "1.1.1",
    "pinia": "^3.0.2",
    "slug": "^11.0.0",
    "vue": "^3.5.14",
    "vue-router": "^4.5.1",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@iconify-json/tabler": "^1.2.23",
    "@tailwindcss/vite": "^4.1.7",
    "@types/slug": "^5.0.9",
    "concurrently": "^9.2.1",
    "daisyui": "^5.1.27",
    "drizzle-kit": "^0.31.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-package-json": "^0.31.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
